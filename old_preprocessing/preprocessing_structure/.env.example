# Rosbag Processing Pipeline Configuration
# Copy this file to .env and update with your actual paths and settings

# Input directory containing rosbag files
ROSBAGS_DIR=/path/to/rosbags

# Output directory for all processed results
OUTPUT_DIR=/path/to/output

# Grid size in meters for positional lookup builder
GRID_SIZE=10.0

# Comma-separated list of embedding models to use
EMBEDDING_MODELS=model1,model2

# Number of embeddings per shard file
SHARD_SIZE=100000
